#!/usr/bin/env python
# -*- coding: utf-8 -*-
#
import sys

from libronan.python.utils import tis


def read_and_tis(path):
    print('here')
    if path.endswith('.pth'):
        import torch
        x = torch.load(path)
    elif path.endswith('.npy'):
        import numpy
        x = numpy.load(path)
    else:
        from PIL import Image
        x = Image.open(path)
    tis(x)

def main():
    list(map(read_and_tis, sys.argv[1:]))

if __name__=='__main__':
    main()
